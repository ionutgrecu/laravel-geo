{
    "name": "ionutgrecu/laravel-geo",
    "description": "A laravel package for geo functions.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Ionut Grecu",
            "email": "ionut@grecu.eu"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Ionutgrecu\\LaravelGeo\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ionutgrecu\\LaravelGeo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ionutgrecu\\LaravelGeo\\LaravelGeoServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
